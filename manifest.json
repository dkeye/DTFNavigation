{
  "manifest_version": 3,
  "name": "DTF.ru Post Navigation",
  "version": "1.0",
  "description": "Allows navigation between posts on DTF.ru",
  "author": "ldifmo@gmail.com",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://dtf.ru/*",
        "https://new.dtf.ru/*"
      ]
    }
  ]
}
